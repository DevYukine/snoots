{
  "name": "snoots",
  "version": "1.0.0-alpha.0",
  "description": "A modern, fully-featured, strongly-typed reddit api.",
  "module": "dist/index.mjs",
  "main": "dist/index.js",
  "types": "./types/index.d.ts",
  "license": "MIT",
  "scripts": {
    "postinstall": "husky install",
    "build": "tsc",
    "test": "jest",
    "clean": "rm -rf dist docs types",
    "lint": "prettier -c '**/*'",
    "gendoc": "typedoc src/index.ts",
    "prepublishOnly": "pinst --disable && npm run clean && npm run lint && npm run build",
    "postpublish": "pinst --enable"
  },
  "dependencies": {
    "camelcase": "^5.3.1",
    "got": "^11.8.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.0.1",
    "@commitlint/config-conventional": "^12.0.1",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/github": "^7.2.0",
    "@semantic-release/npm": "^7.0.10",
    "@semantic-release/release-notes-generator": "^9.0.1",
    "@types/jest": "^26.0.20",
    "fast-check": "^2.13.0",
    "husky": "^5.1.3",
    "jest": "^26.6.3",
    "nock": "^13.0.11",
    "pinst": "^2.1.6",
    "prettier": "2.2.1",
    "semantic-release": "^17.4.1",
    "ts-jest": "^26.5.3",
    "tslib": "^2.1.0",
    "typedoc": "~0.20.30",
    "typedoc-plugin-rename-defaults": "^0.1.0",
    "typescript": "^4.2.3"
  },
  "engines": {
    "node": ">= 14"
  },
  "keywords": [
    "reddit",
    "snoo",
    "snoowrap"
  ],
  "files": [
    "dist",
    "types"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thislooksfun/snoots.git"
  },
  "author": "thislooksfun <tlf@thislooks.fun> (https://thislooks.fun)",
  "bugs": {
    "url": "https://github.com/thislooksfun/snoots/issues"
  },
  "homepage": "https://github.com/thislooksfun/snoots#readme"
}
